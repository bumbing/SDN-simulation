$ vi ginfo
#
#
# Script to print user information who currently login , current date & time
#
clear

# bash until loop
for (( i = 5; i<=50; i+=5)) do
        ./1.out 2000 512 $i 1 0
        ./1.out 2000 512 $i 0 0
done 

for (( i = 5; i<=50; i+=5)) do
        ./1.out 2000 512 $i 1 1
        ./1.out 2000 512 $i 0 1
done 
exit 0
